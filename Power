class Main
{
    public static void main(String args[])
    {
        int num=49;
        for(int i=2;i<num;i++)
        {
            int j=2;
            while(Math.pow(i,j)<=num)
            {
                if(Math.pow(i,j)==num)
                {
                    System.out.println(i + " " + j);
                    break;
                }else
                    j++;
            }
        }
    }
}
